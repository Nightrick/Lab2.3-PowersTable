import java.util.Scanner;

public class PowersTable {
	
	public static void main(String[] args) {
		
		Scanner scnr = new Scanner(System.in);
		
		System.out.print("Hello there! Please enter an integer: ");
		
		int userNum = scnr.nextInt();
		
		for(int i = 1; i <= userNum; i++) {
			System.out.printf("%5s %3d %3d %3d %3d %3d %3d %3d %3d %3d %3d%n", i + " ");
			System.out.printf("%5s %3d %3d %3d %3d %3d %3d %3d %3d %3d %3d%n", i*i + " ");
			System.out.printf("%5s %3d %3d %3d %3d %3d %3d %3d %3d %3d %3d%n", i*i*i + " ");
			System.out.println("");
		}
		
		
	}

}
